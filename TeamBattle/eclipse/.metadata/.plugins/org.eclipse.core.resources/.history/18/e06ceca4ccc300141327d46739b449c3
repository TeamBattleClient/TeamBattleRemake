package down.TeamBattle.event.events;

import down.TeamBattle.event.Event;

public final class EventFOVChange extends Event {
	private float fov;

	public EventFOVChange(float fov) {
		this.fov = fov;
	}

	public float getFOV() {
		return fov;
	}

	public void setFOV(float fov) {
		this.fov = fov;
	}
}
